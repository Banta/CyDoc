- content_for :sidebar do
  .sub-tabs
    %h3= t('cydoc.title.filter')
    %ul
      %li= link_to t('cydoc.actions.all'), url_for(params.merge({:by_value_period => nil, :page => nil}))
      - for year in Booking.fiscal_years
        %li= link_to year, url_for(:params => params.merge({:by_value_period => {:from => Date.new(year.to_i, 1, 1).to_s(:db), :to => Date.new(year.to_i, 12, 31).to_s(:db)}, :page => nil}))
