<tr id="esr_file_<%= item.id %>">
  <td><%= link_to item.updated_at.strftime('%d.%m.%Y'), esr_booking_path(item) %></td>
  <td><%=h item.esr_records.valid.count %></td>
  <td><%=h item.esr_records.invalid.count %></td>
  <td style="text-align: right"><%=h sprintf("%0.2f", item.esr_records.sum(:amount) || 0.0) %></td>
  <td>
    <%= link_to_remote image_tag('delete.png'), {:url => esr_booking_path(item), :method => :delete, :confirm => "VESR Datein inkl. aller Buchungen lÃ¶schen?"} %>
  </td>
</tr>
