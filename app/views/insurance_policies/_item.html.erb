<tr id="insurance_policy_<%= item.id %>">
  <td><%=h item.policy_type %></td>
  <td><%= link_to item.insurance.to_s, item.insurance %></td>
  <td><%=h item.number %></td>
  <td><%= link_to image_tag('delete.png'), {:url => patient_insurance_policy_path(@patient, item), :method => :delete}, :remote => true %></td>
</tr>
