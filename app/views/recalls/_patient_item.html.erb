<tr id="recall_<%= patient_item.id %>">
  <th><%=h patient_item.due_date %></th>
  <td><%=h t('cydoc.recall.appointed_time', :appointment => patient_item.appointment) unless patient_item.appointment.nil? %></td>
  <td><%= t('cydoc.recall.last_appointment') %> <%=link_to_last_session(patient_item) %></td>
  <td><%=h patient_item.sent_at ? t('cydoc.recall.offer_printed', :offer => patient_item.sent_at.to_date) : nil %></td>
  <td><%=h patient_item.remarks %></td>
  <td>
    <%= render :partial => 'recalls/action_links', :locals => {:recall => patient_item} %>
  </td>
</tr>
