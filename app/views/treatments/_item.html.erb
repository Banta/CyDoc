<tr id="invoice_<%= item.id %>">
  <td><%= link_to item.patient.full_name, item.patient %></td>
  <td><%= l(item.date_begin) if item.date_begin%></td>
  <td><%= link_to item.reason, item %></td>
  <td><%= item.law.name if item.law %></td>
  <td style="text-align: right"><%= currency_fmt(item.amount.currency_round) %></td>
</tr>
