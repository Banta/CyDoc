<script type="text/javascript">
//<![CDATA[
Event.observe(window, 'load', function() {$('vcard_honorific_prefix').focus()}, false);
//]]>
</script>

<% semantic_fields_for vcard_name, vcard do |address| %>
  <% address.inputs do %>
    <%= address.input :honorific_prefix, :collection => HonorificPrefix.all.collect { |prefix| prefix.name }, :include_blank => true %>
    <%= address.input :family_name, :size => 23 %>
    <%= address.input :given_name, :size => 23 %>
    <%= address.input :street_address, :size => 23 %>
    <%= address.input :extended_address, :size => 23 %>
    <%= address.input :postal_code, :size => 23 %>
    <%= address.input :locality, :size => 23 %>
  <% end %>
<% end %>
